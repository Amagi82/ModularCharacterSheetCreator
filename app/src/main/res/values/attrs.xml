<resources>
    <declare-styleable name="StatRatingBar">
        <attr name="colorOutlineOn" format="color" />
        <attr name="colorOutlineOff" format="color" />
        <attr name="colorOutlinePressed" format="color" />
        <attr name="colorFill" format="color" />
        <attr name="colorFillPressed" format="color" />
        <attr name="vertical" format="boolean" />
        <attr name="barType" format="enum">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
            <enum name="healthbox" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundedStatBar">
        <attr name="rsb_colorBackground" format="color" />
        <attr name="rsb_colorFill" format="color" />
        <attr name="rsb_colorFillSecondary" format="color" />
        <attr name="rsb_colorBorder" format="color" />
        <attr name="rsb_colorBorderInactive" format="color" />
        <attr name="rsb_textColor" format="color" />
        <attr name="rsb_textSize" format="dimension" />
        <attr name="rsb_ratingMin" format="integer" />
        <attr name="rsb_ratingBase" format="integer" />
        <attr name="rsb_ratingMax" format="integer" />
        <attr name="rsb_gap" format="float" />
        <attr name="rsb_cornerRadius" format="float" />
        <attr name="rsb_title" format="string" />
        <attr name="rsb_specialty" format="string" />
    </declare-styleable>
</resources>
