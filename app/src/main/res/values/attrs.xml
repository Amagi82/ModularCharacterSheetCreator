<resources>
    <declare-styleable name="StatRatingBar">
        <attr name="colorOutlineOn" format="color" />
        <attr name="colorOutlineOff" format="color" />
        <attr name="colorOutlinePressed" format="color" />
        <attr name="colorFill" format="color" />
        <attr name="colorFillPressed" format="color" />
        <attr name="vertical" format="boolean" />
        <attr name="barType" format="enum">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
            <enum name="healthbox" value="2" />
        </attr>
    </declare-styleable>
</resources>