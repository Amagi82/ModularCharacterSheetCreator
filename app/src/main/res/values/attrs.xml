<resources>
    <declare-styleable name="StatRatingBar">
        <attr name="colorOutlineOn" format="color" />
        <attr name="colorOutlineOff" format="color" />
        <attr name="colorOutlinePressed" format="color" />
        <attr name="colorFill" format="color" />
        <attr name="colorFillPressed" format="color" />
        <attr name="vertical" format="boolean" />
        <attr name="barType" format="enum">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
            <enum name="healthbox" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundedRatingBar">
        <attr name="rrb_colorBackground" format="color" />
        <attr name="rrb_colorFill" format="color" />
        <attr name="rrb_colorFillSecondary" format="color" />
        <attr name="rrb_colorBorder" format="color" />
        <attr name="rrb_colorBorderInactive" format="color" />
        <attr name="rrb_textColor" format="color" />
        <attr name="rrb_textSize" format="dimension" />
        <attr name="rrb_ratingMin" format="float" />
        <attr name="rrb_ratingBase" format="float" />
        <attr name="rrb_ratingMax" format="float" />
        <attr name="rrb_gap" format="float" />
        <attr name="rrb_cornerRadius" format="float" />
        <attr name="rrb_title" format="string" />
        <attr name="rrb_specialty" format="string" />
    </declare-styleable>
</resources>
